---
interface Props {
  title: string;
}
const { title } = Astro.props;
---

<header class="h-20 w-full p-10 shadow-lg">
  <div
    class="m-auto flex h-full max-w-[var(--content-width)] items-center gap-16"
  >
    <img src="images/logo.png" class="h-[30px] w-[108px] object-cover" />
    <span class="uppercase">{title}</span>
  </div>
</header>
